{
  "name": "cross-iframe-communication-tutorial",
  "version": "0.0.1",
  "description": "A simple tutorial explaining how to coordinate between multiple iframes on the same page",
  "main": "index.js",
  "scripts": {
    "start-server": "node server.js",
    "start-main": "parcel watch app/main/index.html",
    "start-iframe-3d": "parcel watch app/iframe-3d/index.html --out-dir dist/iframe-3d",
    "start-iframe-control": "parcel watch app/iframe-control/index.html --out-dir dist/iframe-control",
    "start": "npm-run-all --parallel start-server start-main start-iframe-3d start-iframe-control"
  },
  "keywords": [
    "iframe",
    "postMessage",
    "RPC"
  ],
  "author": "Georgi Nikoloff",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
