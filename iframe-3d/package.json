{
  "name": "iframe-3d",
  "version": "0.0.1",
  "description": "Simple iframe that renders a 3D scene with inputs streamed from the parent window",
  "main": "app/index.js",
  "scripts": {
    "start-server": "node server",
    "watch": "parcel watch index.html --out-dir dist/",
    "build": "parcel build index.html --out-dir dist/",
    "start": "npm-run-all --parallel start-server watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "camera-controls": "^1.22.1",
    "express": "^4.17.1",
    "three": "^0.137.0"
  },
  "devDependencies": {
    "glslify-bundle": "^5.1.1",
    "glslify-deps": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4"
  }
}
