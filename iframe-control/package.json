{
  "name": "iframe-control",
  "version": "0.0.1",
  "description": "Simple iframe that caputures the mouse input and propagates it to the parent window",
  "main": "app/index.js",
  "scripts": {
    "start-server": "nodemon server",
    "watch": "parcel watch index.html --out-dir dist/",
    "build": "parcel build index.html --out-dir dist/",
    "start": "npm-run-all --parallel start-server watch"
  },
  "author": "Georgi Nikoloff",
  "keywords": [
    "iframe",
    "postMessage",
    "RPC",
    "iframe-communication"
  ],
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4"
  }
}
