{
  "name": "cross-iframe-communication-tutorial",
  "version": "0.0.1",
  "description": "Topframe adapter that glues multiple iframe and acts as a coordinator between them",
  "main": "app/index.js",
  "scripts": {
    "start-server": "nodemon server.js",
    "watch": "parcel watch index.html",
    "build": "parcel build index.html --out-dir dist/",
    "start": "npm-run-all --parallel start-server watch"
  },
  "keywords": [
    "iframe",
    "postMessage",
    "RPC",
    "iframe-communication"
  ],
  "author": "Georgi Nikoloff",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "parcel": "^2.8.2"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
